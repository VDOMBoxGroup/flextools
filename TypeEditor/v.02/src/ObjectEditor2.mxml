<?xml version="1.0" encoding="utf-8"?>
<s:WindowedApplication xmlns:fx="http://ns.adobe.com/mxml/2009" 
					   xmlns:s="library://ns.adobe.com/flex/spark" 
					   xmlns:mx="library://ns.adobe.com/flex/mx"
					   xmlns:ns="net.vdombox.object_editor.view.essence.*"
					   xmlns:vs="net.vdombox.object_editor.view.*" 
					   minWidth="900" minHeight="600">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects)  -->
	</fx:Declarations>
	<s:HGroup width="100%" height="100%" >
		<mx:VBox width="100%" height="100%" >
			<s:Button label="Open"/>
		<mx:Accordion width="150" height="100%" >
			<s:NavigatorContent label="Accordion Button for Panel 1">
				<mx:Label id="labll" text="Accordion container panel 1"/>
				
			</s:NavigatorContent>
			
			<s:NavigatorContent label="Accordion Button for Panel 2">
				<mx:Label text="Accordion container panel 2"/>
			</s:NavigatorContent>
		</mx:Accordion>
		</mx:VBox>
		<mx:TabNavigator  width="100%" height="100%">
			
			<s:NavigatorContent label="HTMLContainer">
				<vs:ObjectView/>
			</s:NavigatorContent>	
			
			<s:NavigatorContent label="Form">
				<vs:ObjectView/>
			</s:NavigatorContent>	
			
			<s:NavigatorContent label="Button">
				<vs:ObjectView/>
			</s:NavigatorContent>
			
			<s:NavigatorContent label="Button2">
				<vs:ObjectView/>
			</s:NavigatorContent>
			
			
			
			<s:NavigatorContent label="Copy">
				<vs:ObjectView/>
			</s:NavigatorContent>	
		</mx:TabNavigator>
	</s:HGroup>
</s:WindowedApplication>
