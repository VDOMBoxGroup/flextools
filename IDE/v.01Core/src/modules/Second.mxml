<?xml version="1.0" encoding="utf-8"?>
<VDOMIDEModule 
	xmlns:mx="http://www.adobe.com/2006/mxml"
	
	height="100%" layout="absolute" width="100%"
	
	borderColor="#000000" borderStyle="solid"
	xmlns="modules.*">
	<mx:Script>
		<![CDATA[
			import mx.core.UIComponent;
			override public function get title() : String
			{
				return "22222222";
			}
			
			public function get toolset() : UIComponent
			{
				var toolsetButton : Button = new Button();
				toolsetButton.label = title;
				return toolsetButton;
			}
		]]>
	</mx:Script>
	<mx:Button 
		label="Second module" x="10" y="10"/>
	
</VDOMIDEModule>
