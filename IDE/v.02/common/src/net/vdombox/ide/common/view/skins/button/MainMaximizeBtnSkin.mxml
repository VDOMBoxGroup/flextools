<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
			 xmlns:s="library://ns.adobe.com/flex/spark"
			 minWidth="10" minHeight="10"
			 height="12" width="12" xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")] 
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			import net.vdombox.ide.common.view.components.VDOMImage;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<s:SolidColorStroke id="stroke1"
							color="#ffffff"
							weight="2"/>
		
		<s:SolidColorStroke id="stroke2"
							color="#ffffff"
							weight="1"/>
		
		<s:SolidColorStroke id="stroke12"
							color="#ffff00"
							weight="2"/>
		
		<s:SolidColorStroke id="stroke22"
							color="#ffff00"
							weight="1"/>
	</fx:Declarations>
	
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	
	<mx:Image width="100%" height="100%" source="{ VDOMImage.Clear }"/>
	
	<!-- vertical lines -->
	<s:Line xFrom="9.5" yFrom="2" xTo="9.5" yTo="10" stroke="{stroke2}" stroke.over="{stroke22}"/>		<!-- right -->
	<s:Line xFrom="0.5" yFrom="2" xTo="0.5" yTo="10" stroke="{stroke2}" stroke.over="{stroke22}"/>		<!-- left -->
	
	<!-- horizontal lines -->
	<s:Line xFrom="0.5" yFrom="1" xTo="9" yTo="1" stroke="{stroke1}" stroke.over="{stroke12}"/> 			<!-- top -->
	<s:Line xFrom="0.5" yFrom="9" xTo="9.5" yTo="9" stroke="{stroke2}" stroke.over="{stroke22}"/>		<!-- bottom -->
	
</s:SparkSkin>
