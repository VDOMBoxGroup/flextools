<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:controls="ExtendedAPI.com.controls.*">

	<mx:Script>
		<![CDATA[
			import PowerPack.com.managers.ContextManager;
			import mx.events.ItemClickEvent;
			import PowerPack.com.managers.LanguageManager;
			
			static private var defaultCaptions:Object = {	
				importation_complete: "Importation complete"
			}			
			
			static private var _classConstructed:Boolean = classConstruct(); 
			
			static private function classConstruct():Boolean
			{
				LanguageManager.setSentences(defaultCaptions);
				return true
			}	
            
		]]>
	</mx:Script>

	<mx:Grid width="100%">
		<mx:GridRow>
			<mx:GridItem colSpan="2">
				<mx:Label text="{LanguageManager.sentences.importation_complete}"/>
			</mx:GridItem>
		</mx:GridRow>
	</mx:Grid>	
	
			
</mx:VBox>
