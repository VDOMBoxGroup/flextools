<language label="english" id="EN">
	<sentense id="" label="console_title">Console</sentense>
	<sentense id="" label="menu_file">_File</sentense>
	
	<designpanel>
		<title>Power Pack Graph Design</title>		
	</designpanel>
	
	<navigatorpanel>
		<title>VDOM Application</title>
		<graphs>	
			<title>Application</title>
			<btnnew>New</btnnew>
			<btnrename>Rename</btnrename>
			<btndelete>Delete</btndelete>
			<btnhelp>Help</btnhelp>
		</graphs>	
	</navigatorpanel>
	
	<initialgraphpanel>
		<title>Initial graph</title>	
	</initialgraphpanel>

    <mainmenu>
        <menuitem label="File">
            <menuitem label="New Template" id="new"/>
            <menuitem label="New Tab" id="new_tab"/>
            <menuitem label="Open File..." id="open_file"/>
            <menuitem type="separator"/>
            <menuitem label="Close" id="close"/>
            <menuitem type="separator"/>
            <menuitem label="Save" id="save"/>
            <menuitem label="Save As..." id="save_as"/> 
            <menuitem type="separator"/>
            <menuitem label="Exit" id="exit"/>                                                
        </menuitem>
        <menuitem label="Settings" id="settings">
            <menuitem label="Language">        
            	<menuitem label="English" id="language" data="english.xml" type="radio" groupName="language"  toggled="true"/>
            	<menuitem label="Russian" id="language" data="russian.xml" type="radio" groupName="language"/>
	        </menuitem>        
        </menuitem>        
        <menuitem label="Help" data="help">
        </menuitem>
        <menuitem label="Generator">
        	<menuitem label="Run" id="generate"/>
        </menuitem>
    </mainmenu>
    
    <filebrowse>
     	<select>
    		<title>Select File</title>
    		<btnselect>Select</btnselect>
    		<btncancel>Cancel</btncancel>    		
    	</select>
        <open>
    		<title>Open File</title>
    		<btnopen>Open</btnopen>
    		<btncancel>Cancel</btncancel>   		
    	</open>
    	<save>
    		<title>Save File</title>
    		<btnsave>Save</btnsave>
    		<btncancel>Cancel</btncancel>   		
    	</save>
   		<lblfilename>File Name:</lblfilename>   		
   		<lblfiletype>File Type:</lblfiletype>  		
    </filebrowse>
    
    <grapharrow>
    	<alertdeletetitle>Confirmation</alertdeletetitle>
    	<alertdeletetext>Are you sure want to delete this arrow?</alertdeletetext>
    	<defaultlabel>Arrow</defaultlabel>
    	
    	<menuitemselecttrans>Select Transition</menuitemselecttrans>
    	<menuitemdelete>Delete Arrow</menuitemdelete>
    	<menuitemenable>Enable Arrow</menuitemenable>
    	<menuitemhighlight>Highlight Arrow</menuitemhighlight>
    </grapharrow>
    
    <graphnode>
    	<alertdeletetitle>Confirmation</alertdeletetitle>
    	<alertdeletetext>Are you sure want to delete this node?</alertdeletetext>
    	<defaultlabel>Node</defaultlabel>
    	
    	<menuitemaddtrans>Add Transition</menuitemaddtrans>
    	<menuitemdelete>Delete Node</menuitemdelete>
    	<menuitemcopy>Copy Node</menuitemcopy>
    	<menuiteminitial>Initial</menuiteminitial>
    	<menuitemterminal>Terminal</menuitemterminal>
    	<menuitemnormal>Normal</menuitemnormal>
    	<menuitemsubgraph>Sub Graph</menuitemsubgraph>
    	<menuitemcommand>Command</menuitemcommand>
    </graphnode>  

	<graphcanvas>
		<menuitemadd>Add State</menuitemadd>		
		<menuitempaste>Paste State</menuitempaste>
		<menuitemclear>Clear Stage</menuitemclear>
	</graphcanvas> 

</language>